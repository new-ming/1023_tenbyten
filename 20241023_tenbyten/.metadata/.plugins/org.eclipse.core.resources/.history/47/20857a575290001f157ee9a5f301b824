		int board_no = Integer.getInteger( request.getParameter("board_no"));
		
		ServiceBoardDAO sbDao = ServiceBoardDAO.getInstance();
		ServiceBoardVO sbVo = sbDao.getServiceBoardView(board_no);
		

	}

}
